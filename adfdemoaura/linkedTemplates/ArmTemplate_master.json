{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Nom de fabrique de données",
			"defaultValue": "adfdemoaura"
		},
		"trigger1_properties_typeProperties_recurrence_frequency": {
			"type": "string",
			"defaultValue": "Day"
		},
		"trigger1_properties_typeProperties_recurrence_interval": {
			"type": "int",
			"defaultValue": 1
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI pour le compte de stockage contenant des modèles ARM liés"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Jeton SAS pour le compte de stockage contenant des modèles ARM liés"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfdemoaura_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"trigger1_properties_typeProperties_recurrence_frequency": {
						"value": "[parameters('trigger1_properties_typeProperties_recurrence_frequency')]"
					},
					"trigger1_properties_typeProperties_recurrence_interval": {
						"value": "[parameters('trigger1_properties_typeProperties_recurrence_interval')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}